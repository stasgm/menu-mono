{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "🚀 dev",
      "type": "shell",
      "command": "pnpm",
      "args": ["dev"],
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {
          "NODE_ENV": "dev"
        }
      }
    },
    {
      "label": "🌿 seed",
      "command": "pnpm",
      "args": ["seed"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "🗃️ prisma:generate",
      "command": "pnpm",
      "args": ["run", "prisma:generate"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "📤 migrate:dev",
      "command": "pnpm",
      "args": ["migrate:dev"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "📥 migrate:create",
      "command": "pnpm",
      "args": ["migrate:dev:create"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "✨ Format",
      "command": "pnpm",
      "args": ["format"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
    {
      "label": "🧶 lint",
      "command": "pnpm",
      "args": ["lint"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}
