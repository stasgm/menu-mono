{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleNameMapper": {
    "^@/core(.*)$": "<rootDir>/core$1",
    "^@/modules(.*)$": "<rootDir>/modules$1",
    "^@/types(.*)$": "<rootDir>/types$1",
    "^@/utils(.*)$": "<rootDir>/utils$1"
  },
  "rootDir": "test",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(ts|js)?$": [
      "@swc/jest"
    ]
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
