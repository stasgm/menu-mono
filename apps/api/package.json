{
  "private": true,
  "name": "@apps/api",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d --build",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml --env-file .env.docker up -d --build",
    "docker:seed": "docker exec -it menu-prisma-migrate pnpm run seed",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "graphql:generate": "ts-node generate-typings",
    "postinstall": "pnpm run prisma:generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-staged": "pnpm dlx lint-staged",
    "migrate:deploy": "pnpm dlx prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:status": "pnpm dlx prisma migrate status",
    "prisma:generate": "pnpm dlx prisma generate",
    "seed": "prisma db seed",
    "start": "nest start",
    "start:db": "pnpm run migrate:dev && pnpm run prisma:generate && pnpm run seed",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:watch": "jest --watch --runInBand",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@graphql-tools/utils": "^10.0.5",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/swagger": "^7.1.8",
    "@packages/domains": "workspace:*",
    "@packages/mocks": "workspace:*",
    "@packages/utils": "workspace:*",
    "@prisma/client": "^5.1.1",
    "graphql": "^16.7.1",
    "prisma": "^5.1.1",
    "prisma-nestjs-graphql": "^18.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "ts-morph": "^19.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@configs/lint-staged": "workspace:*",
    "@configs/nest-eslint": "workspace:*",
    "@configs/nest-prettier": "workspace:*",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "^20.4.10",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.2",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
