    "@packages/utils": "workspace:*",
    "@prisma/client": "^5.10.2",
    "@types/config": "^3.3.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.4.1",
    "cache-manager-redis-store": "^3.0.1",
