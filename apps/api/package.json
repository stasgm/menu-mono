{
  "private": true,
  "name": "@apps/api",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d --build",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml --env-file .env.docker up -d --build",
    "docker:seed": "docker exec -it menu-prisma-migrate pnpm run seed",
    "format": "prettier --write \"{src,apps,libs,test,seeds}/**/*.ts\"",
    "postinstall": "pnpm run prisma:generate",
    "lint": "eslint \"{src,apps,libs,test,seeds}/**/*.ts\" --fix",
    "lint-staged": "pnpm dlx lint-staged",
    "migrate:deploy": "pnpm dlx prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:status": "pnpm dlx prisma migrate status",
    "migrate:test": "dotenv -e .env.test prisma migrate dev",
    "prisma:generate": "pnpm dlx prisma generate",
    "seed": "prisma db seed",
    "seed:test": "dotenv -e .env.test prisma db seed",
    "start": "nest start",
    "start:db": "pnpm run migrate:dev && pnpm run prisma:generate && pnpm run seed",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/src/main",
    "test": "jest --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --detectOpenHandles",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
