    "@configs/nest-prettier": "workspace:*",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "eslint": "8.44.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
