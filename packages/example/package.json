{
  "private": true,
  "name": "@packages/example",
  "version": "0.0.1",
  "description": "Example that imports a local package",
  "main": "lib",
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rimraf lib",
    "format:check": "prettier --parser=typescript \"src/**/*.{js,ts}\" --check",
    "format:write": "prettier --parser=typescript \"src/**/*.{js,ts}\" --write",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint-staged": "lint-staged",
    "pretest": "jest --clearCache",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "eslint": "8.44.0"
  }
}
